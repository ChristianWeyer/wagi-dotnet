{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Deislabs.WAGI": "Trace"
    }
  },
  "AllowedHosts": "*",
  "WASM": {
    "ModulePath": "modules",
    "Modules": {
      "wagi.project": {
        "FileName": "wasm_filename",
        "Entrypoint": "wasm_entrypoint",
        "Environment" :{
          wasm_environment
        },
        "Volumes" : {
          wasm_volumes
        },
        "Route": "path_pattern",
        "HttpMethod": "wagi_method"
      }
    }
  }
}
