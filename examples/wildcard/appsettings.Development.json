{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Deislabs.WAGI": "Trace"
    }
  },
  "Wagi": {
    "ModulePath": "modules",
    "Modules": {
        "/path": {
            "FileName": "env_wagi.wasm",
            "Volumes": {
                "/": "files"
            },
            "Environment": {
                "ROUTE_VALUE": "/path"
            },
            "Route": "/path"
        },
        "/path/...": {
            "FileName": "env_wagi.wasm",
            "Volumes": {
                "/": "files"
            },
            "Environment": {
                "ROUTE_VALUE": "/path/..."
            },
            "Route": "/path/..."
        },
        "/...": {
            "FileName": "env_wagi.wasm",
            "Volumes": {
                "/": "files"
            },
            "Environment": {
                "ROUTE_VALUE": "/..."
            },
            "Route": "/..."
        }
        
    }
  }
}
