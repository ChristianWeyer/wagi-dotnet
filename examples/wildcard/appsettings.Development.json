{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Deislabs.WAGI": "Trace"
    }
  },
  "WASM": {
    "ModulePath": "modules",
    "Modules": {
      "/path": {
        "FileName": "env_wagi.wasm",
        "Volumes": {
            "/":"files"
          },
          "Environment" :{
            "ROUTE_VALUE":"/path"
          }
      },
      "/path/...": {
        "FileName": "env_wagi.wasm",
        "Volumes": {
            "/":"files"
          },
          "Environment" :{
            "ROUTE_VALUE":"/path/..."
          }
      },
      "/...": {
        "FileName": "env_wagi.wasm",
        "Volumes": {
            "/":"files"
          },
          "Environment" :{
            "ROUTE_VALUE":"/..."
          }
      }
    }
  }
}
